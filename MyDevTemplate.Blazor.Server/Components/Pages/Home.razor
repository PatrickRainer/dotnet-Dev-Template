@page "/"
@using MyDevTemplate.Blazor.Server.Infrastructure
@inject NavigationManager NavigationManager

<PageTitle>Home</PageTitle>

<MudContainer>
    <MudCard>
        <MudCardHeader>@Ressources.Blazor_Server_Localization.common_Register</MudCardHeader>
        <MudCardContent>
            Start with your Company Registration
